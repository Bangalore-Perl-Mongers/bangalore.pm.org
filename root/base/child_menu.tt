<ul class="child-menu">
    [%- FOR child_page = parent_page.children %]
    <li id="menupage[% child_page.id %]"
        [% IF (child_page.rgt-child_page.lft) > 1 %]
            class="menuParentMissingChildren"
        [% END %]>
        <a href="[% base _ child_page.path %]">[% child_page.name_orig %]</a>
    </li>
    [%- END %]
</ul>
